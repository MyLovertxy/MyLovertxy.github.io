<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  		<servlet-name>loginServlet</servlet-name>
  		<servlet-class>com.wuyong.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>loginServlet</servlet-name>
  		<url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
  		<servlet-name>JdbcDemo</servlet-name>
  		<servlet-class>com.wuyong.servlet.JdbcDemo</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>JdbcDemo</servlet-name>
  		<url-pattern>/JdbcDemo</url-pattern>
  </servlet-mapping>
  <servlet>
  		<servlet-name>RegisterServlet</servlet-name>
  		<servlet-class>com.wuyong.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>RegisterServlet</servlet-name>
  		<url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet>
	    <servlet-name>querySingleEmp</servlet-name>
	    <servlet-class>com.wuyong.servlet.querySingleEmp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>querySingleEmp</servlet-name>
    <url-pattern>/querySingleEmp</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>deleteServlet</servlet-name>
    <servlet-class>com.wuyong.servlet.deleteServlet</servlet-class>
  </servlet>
<servlet-mapping>
    <servlet-name>deleteServlet</servlet-name>
    <url-pattern>/deleteServlet</url-pattern>
  </servlet-mapping>
<servlet>
    <servlet-name>OutLoginServlet</servlet-name>
    <servlet-class>com.wuyong.servlet.OutLoginServlet</servlet-class>
  </servlet>
<servlet-mapping>
    <servlet-name>OutLoginServlet</servlet-name>
    <url-pattern>/outLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InsertServlet</servlet-name>
    <servlet-class>com.wuyong.servlet.InsertServlet</servlet-class>
  </servlet>
<servlet-mapping>
    <servlet-name>InsertServlet</servlet-name>
    <url-pattern>/InsertServlet</url-pattern>
  </servlet-mapping>

</web-app>
